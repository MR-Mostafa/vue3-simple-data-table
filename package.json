{
	"name": "tablevue",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"clean": "rimraf \"./dist\"",
		"dev": "yarn run clean && vite",
		"build": "yarn run clean && vue-tsc && vite build",
		"preview": "yarn run build && vite preview",
		"prettier:check": "prettier --check \"src/**/*.{ts,js,vue}\"",
		"prettier:fix": "prettier --write \"src/**/*.{ts,js,vue}\"",
		"eslint:check": "eslint \"src/**/*.{ts,js,vue}\"",
		"eslint:fix": "eslint --fix \"src/**/*.{ts,js,vue}\"",
		"spell": "cspell --no-progress \"src/**/*\"",
		"pre-commit": "yarn run prettier:check && yarn run eslint:check && yarn run spell",
		"lint": "yarn run pre-commit",
		"prepare": "husky install"
	},
	"dependencies": {
		"axios": "^1.4.0",
		"bootstrap": "^5.2.3",
		"uuid": "^9.0.0",
		"vue": "^3.2.47"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/node": "^20.2.1",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"@vitejs/plugin-vue": "^4.1.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"cspell": "^6.31.1",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.13.0",
		"husky": "^8.0.3",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"sass": "^1.62.1",
		"typescript": "^5.0.2",
		"vite": "^4.3.2",
		"vue-eslint-parser": "^9.3.0",
		"vue-tsc": "^1.4.2"
	}
}
